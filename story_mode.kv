<StoryMode>:
    background_color: (0, 0, 0, 0)
    do_default_tab: False

<Area>:
    padding: "30dp"
    size_hint: None, 1
    width: self.minimum_width

<Level>:
    size_hint: (None, None)
    size: ("150dp", "150dp")
    font_size: self.width / 4
    color: "#A04623"
    font_name: "fonts/Coda-Regular.ttf"
    background_normal: "images/buttons/level.png"
    background_down: "images/buttons/level.png"

<MyBackgroundImage>:
    size_hint: (1, 1)

<TabItem>:
    font_name: "fonts/Coda-Regular.ttf"
    background_color: (1, 1, 1, 1)
    border: [0, 0, 0, 0]
    background_normal: "images/buttons/tab.png"
    background_down: "images/buttons/tab.png"